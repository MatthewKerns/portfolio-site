{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AmplifyReadAccess",
      "Effect": "Allow",
      "Action": [
        "amplify:GetApp",
        "amplify:GetBranch",
        "amplify:ListApps",
        "amplify:ListBranches",
        "amplify:GetBackendEnvironment",
        "amplify:ListBackendEnvironments"
      ],
      "Resource": [
        "arn:aws:amplify:us-east-1:836071698725:apps/d2acyy7momhrpf",
        "arn:aws:amplify:us-east-1:836071698725:apps/d2acyy7momhrpf/*"
      ]
    },
    {
      "Sid": "AmplifyDeploymentAccess",
      "Effect": "Allow",
      "Action": [
        "amplify:StartJob",
        "amplify:GetJob",
        "amplify:ListJobs",
        "amplify:StopJob"
      ],
      "Resource": [
        "arn:aws:amplify:us-east-1:836071698725:apps/d2acyy7momhrpf",
        "arn:aws:amplify:us-east-1:836071698725:apps/d2acyy7momhrpf/*"
      ]
    },
    {
      "Sid": "CloudWatchLogsReadAccess",
      "Effect": "Allow",
      "Action": [
        "logs:DescribeLogGroups",
        "logs:DescribeLogStreams",
        "logs:GetLogEvents",
        "logs:FilterLogEvents",
        "logs:StartLiveTail",
        "logs:StopLiveTail",
        "logs:TailLogs"
      ],
      "Resource": [
        "arn:aws:logs:us-east-1:836071698725:log-group:/aws/amplify/d2acyy7momhrpf*:*"
      ]
    }
  ]
}
